{
    "compilerOptions": {
        "moduleResolution": "node",
        "allowJs": true,
        "outDir": "./build/",
        "sourceMap": true,
        "noImplicitAny": false,
        "declaration": false,
        "esModuleInterop": true,
        "skipLibCheck": true,
        "target": "es5",
        "lib": ["es2015", "es2016", "es2017", "dom"]
    },
    "include": ["./http2/**/*", "./http3/**/*"]
}
